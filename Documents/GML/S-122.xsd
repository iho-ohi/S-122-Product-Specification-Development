					<xs:documentation>Pertaining to environmental protection.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Wildlife Protection">
				<xs:annotation>
					<xs:documentation>Pertaining to wildlife protection.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Security">
				<xs:annotation>
					<xs:documentation>Pertaining to security.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Customs">
				<xs:annotation>
					<xs:documentation>The agency or establishment for collecting duties, tolls.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Cargo Operation">
				<xs:annotation>
					<xs:documentation>Pertaining to cargo operations.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Refuge">
