			</xs:element>
			<xs:element name="reportedDate" type="S100_TruncatedDate" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The date that the item was observed, done, or investigated.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="timeIntervalsByDayOfWeekType">
		<xs:annotation>
			<xs:documentation>The regular weekly operation times of a service or schedule.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="dayOfWeek" type="dayOfWeekType" minOccurs="0" maxOccurs="7">
