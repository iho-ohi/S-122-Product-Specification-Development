	<xs:simpleType name="telecommunicationServiceType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Voice">
				<xs:annotation>
					<xs:documentation>The transfer or exchange of information by using sounds that are being made by mouth and throat when speaking.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Facsimile">
				<xs:annotation>
					<xs:documentation>A system of transmitting and reproducing graphic matter (as printing or still pictures) by means of signals sent over telephone lines.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SMS">
				<xs:annotation>
					<xs:documentation>Short Message Service is a form of text messaging communication on phones and mobile phones.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Data">
				<xs:annotation>
					<xs:documentation>A representation of facts, concepts or instructions in a formalised manner suitable for communication, interpretation or processing.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Streamed Data">
				<xs:annotation>
					<xs:documentation>Data that is constantly received by and presented to an end-user while being delivered by a provider.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Telex">
				<xs:annotation>
					<xs:documentation>A system of communication in which messages are sent over long distances by using a telephone system and are printed by using a special machine (called a teletypewriter).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Telegraph">
				<xs:annotation>
					<xs:documentation>An apparatus, system or process for communication at a distance by electric transmission over wire.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Email">
				<xs:annotation>
					<xs:documentation>Messages and other data exchanged between individuals using computers in a network.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="categoryOfScheduleType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Normal Operation">
				<xs:annotation>
					<xs:documentation>The service, office, is open, fully manned, and operating normally, or the area is accessible as usual.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Closure">
				<xs:annotation>
					<xs:documentation>The service, office, or area is closed.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Unmanned Operation">
				<xs:annotation>
					<xs:documentation>The service is available but not manned.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="categoryOfDangerousOrHazardousCargoType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="IMDG Code Class 1 Div. 1.1">
				<xs:annotation>
					<xs:documentation>Explosives, Division 1: Substances and articles which have a mass explosion hazard.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="IMDG Code Class 1 Div. 1.2">
				<xs:annotation>
					<xs:documentation>Explosives, Division 2: Substances and articles which have a projection hazard but not a mass explosion hazard.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="IMDG Code Class 1 Div. 1.3">
				<xs:annotation>
					<xs:documentation>Explosives, Division 3: Substances and articles which have a fire hazard and either a minor blast hazard or a minor projection hazard or both, but not a mass explosion hazard.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="IMDG Code Class 1 Div. 1.4">
				<xs:annotation>
					<xs:documentation>Explosives, Division 4: Substances and articles which present no significant hazard.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="IMDG Code Class 1 Div. 1.5">
				<xs:annotation>
					<xs:documentation>Explosives, Division 5: Very insensitive substances which have a mass explosion hazard.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="IMDG Code Class 1 Div. 1.6">
				<xs:annotation>
					<xs:documentation>Explosives, Division 6: Extremely insensitive articles which do not have a mass explosion hazard.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="IMDG Code Class 2 Div. 2.1">
				<xs:annotation>
					<xs:documentation>Gases, flammable gases.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="IMDG Code Class 2 Div. 2.2">
				<xs:annotation>
					<xs:documentation>Gases, non-flammable, non-toxic gases.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="IMDG Code Class 2 Div. 2.3">
				<xs:annotation>
					<xs:documentation>Gases, toxic gases.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="IMDG Code Class 3">
				<xs:annotation>
					<xs:documentation>Flammable liquids.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="IMDG Code Class 4 Div. 4.1">
				<xs:annotation>
					<xs:documentation>Flammable solids, self-reactive substances and desensitized explosives.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="IMDG Code Class 4 Div. 4.2">
				<xs:annotation>
					<xs:documentation>Substances liable to spontaneous combustion.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="IMDG Code Class 4 Div. 4.3">
				<xs:annotation>
					<xs:documentation>Substances which, in contact with water, emit flammable gases.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="IMDG Code Class 5 Div. 5.1">
				<xs:annotation>
					<xs:documentation>Oxidizing substances.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="IMDG Code Class 5 Div. 5.2">
				<xs:annotation>
					<xs:documentation>Organic peroxides.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="IMDG Code Class 6 Div. 6.1">
				<xs:annotation>
					<xs:documentation>Toxic substances.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="IMDG Code Class 6 Div. 6.2">
				<xs:annotation>
					<xs:documentation>Infectious substances.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="IMDG Code Class 7">
				<xs:annotation>
					<xs:documentation>Radioactive material.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="IMDG Code Class 8">
				<xs:annotation>
					<xs:documentation>Corrosive substances.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="IMDG Code Class 9">
				<xs:annotation>
					<xs:documentation>Miscellaneous dangerous substances and articles.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Harmful Substances in Packaged Form">
				<xs:annotation>
					<xs:documentation>Harmful substances are those substances which are identified as marine pollutants in the International Maritime Dangerous Goods Code (IMDG Code). Packaged form is defined as the forms of containment specified for harmful substances in the IMDG Code.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="categoryOfCargoType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Bulk">
				<xs:annotation>
					<xs:documentation>Unpacked homogenous cargo poured loose in a certain space of a vessel e.g. oil or grain.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Container">
				<xs:annotation>
					<xs:documentation>One of a number of standard sized cargo carrying units, secured using standard corner attachments and bar.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="General">
				<xs:annotation>
					<xs:documentation>Break bulk cargo normally loaded by crane.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Liquid">
				<xs:annotation>
					<xs:documentation>Any cargo loaded by pipeline.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Passenger">
				<xs:annotation>
					<xs:documentation>A fee paying traveller.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Livestock">
				<xs:annotation>
					<xs:documentation>Live animals carried in bulk.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Dangerous or Hazardous">
				<xs:annotation>
					<xs:documentation>Dangerous or hazardous cargo as described by the IMO International Maritime Dangerous Goods code.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Heavy Lift">
				<xs:annotation>
					<xs:documentation>Indivisible heavy items of weight generally over 100 tons, and width or height greater than 100 metres.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Ballast">
				<xs:annotation>
					<xs:documentation>Material carried by a ship to ensure its stability.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="categoryOfCommunicationPreferenceType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Preferred Calling">
				<xs:annotation>
					<xs:documentation>The first choice channel or frequency to be used when calling a radio station.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Alternate Calling">
				<xs:annotation>
					<xs:documentation>A channel or frequency to be used for calling a radio station when the preferred channel or frequency is busy or is suffering from interference.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Preferred Working">
				<xs:annotation>
					<xs:documentation>The first choice channel or frequency to be used when working with a radio station.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Alternate Working">
				<xs:annotation>
					<xs:documentation>A channel or frequency to be used for working with a radio station when the preferred working channel or frequency is busy or is suffering from interference.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="categoryOfAuthorityType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Border Control">
				<xs:annotation>
					<xs:documentation>The administration to prevent or detect and prosecute violations of rules and regulations at international boundaries.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Police">
				<xs:annotation>
					<xs:documentation>The department of government, or civil force, charged with maintaining public order.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Port">
				<xs:annotation>
					<xs:documentation>Person or corporation, owners of, or entrusted with or invested with the power of managing a port. May be called a Harbour Board, Port Trust, Port Commission, Harbour Commission, Marine Department.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Immigration">
				<xs:annotation>
					<xs:documentation>The authority controlling people entering a country.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Health">
				<xs:annotation>
					<xs:documentation>The authority with responsibility for checking the validity of the health declaration of a vessel and for declaring free pratique.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Coast Guard">
				<xs:annotation>
					<xs:documentation>Organization keeping watch on shipping and coastal waters according to governmental law; normally the authority with responsibility for search and rescue.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Agricultural">
				<xs:annotation>
					<xs:documentation>The authority with responsibility for preventing infection of the agriculture of a country and for the protection of the agricultural interests of a country.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Military">
				<xs:annotation>
					<xs:documentation>A military authority which provides control of access to or approval for transit through designated areas or airspace.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Private Company">
				<xs:annotation>
					<xs:documentation>A private or publicly owned company or commercial enterprise which exercises control of facilities, for example a calibration area.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Maritime Police">
				<xs:annotation>
					<xs:documentation>A governmental or military force with jurisdiction in territorial waters. Examples could include Gendarmerie Maritime, Carabinierie, and Guardia Civil.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Environmental">
				<xs:annotation>
					<xs:documentation>An authority with responsibility for the protection of the environment.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Fishery">
				<xs:annotation>
					<xs:documentation>An authority with responsibility for the control of fisheries.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Finance">
				<xs:annotation>
					<xs:documentation>An authority with responsibility for the control and movement of money.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Maritime">
				<xs:annotation>
					<xs:documentation>A national or regional authority charged with administration of maritime affairs.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Customs">
				<xs:annotation>
					<xs:documentation>The agency or establishment for collecting duties, tolls.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="categoryOfVesselTrafficServiceType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Information Service">
				<xs:annotation>
					<xs:documentation>A service to ensure that essential information becomes available in time for on-board navigational decision-making.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Traffic Organization Service">
				<xs:annotation>
					<xs:documentation>A service to assist on-board navigational decision-making and to monitor its effects.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Navigational Assistance Service">
				<xs:annotation>
					<xs:documentation>A service to prevent the development of dangerous maritime traffic situations and to provide for the safe and efficient movement of vessel traffic within the VTS area.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Ship Reporting Service">
				<xs:annotation>
					<xs:documentation>A service established by a relevant authority consisting of one or more reporting points or lines at which ships are required to report their identity, course, speed and other data to the monitoring authority.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Local Port Service">
				<xs:annotation>
					<xs:documentation>A service established to provide port information without interaction between the customer and the service provider. This information could be inter-alia berthing information, availability of port services, shipping schedules, meteorological and hydrological data.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="statusType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Permanent">
				<xs:annotation>
					<xs:documentation>Intended to last or function indefinitely.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Occasional">
				<xs:annotation>
					<xs:documentation>Acting on special occasions; happening irregularly.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Recommended">
				<xs:annotation>
					<xs:documentation>Presented as worthy of confidence, acceptance, use, etc.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Not in Use">
				<xs:annotation>
					<xs:documentation>Use has ceased, but the facility still exists intact; disused.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Periodic/Intermittent">
				<xs:annotation>
					<xs:documentation>Recurring at intervals.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Reserved">
				<xs:annotation>
					<xs:documentation>Set apart for some specific use.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Temporary">
				<xs:annotation>
					<xs:documentation>Meant to last only for a time.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Private">
				<xs:annotation>
					<xs:documentation>Administered by an individual or corporation, rather than a State or a public body.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Mandatory">
				<xs:annotation>
					<xs:documentation>Compulsory; enforced.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Extinguished">
				<xs:annotation>
					<xs:documentation>No longer lit.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Illuminated">
				<xs:annotation>
					<xs:documentation>Lit by floodlights, strip lights, etc.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Historic">
				<xs:annotation>
					<xs:documentation>Famous in history; of historical interest.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Public">
				<xs:annotation>
					<xs:documentation>Belonging to, available to, used or shared by, the community as a whole and not restricted to private use.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Synchronized">
				<xs:annotation>
					<xs:documentation>Occur at a time, coincide in point of time, be contemporary or simultaneous.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Watched">
				<xs:annotation>
					<xs:documentation>Looked at or observed over a period of time especially so as to be aware of any movement or change.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Unwatched">
				<xs:annotation>
					<xs:documentation>Usually automatic in operation, without any permanently-stationed personnel to superintend it.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Existence Doubtful">
				<xs:annotation>
					<xs:documentation>A feature that has been reported but has not been definitely determined to exist.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="On Request">
				<xs:annotation>
					<xs:documentation>When you ask for it.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Drop Away">
				<xs:annotation>
					<xs:documentation>To become lower in level.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Rising">
				<xs:annotation>
					<xs:documentation>To become higher in level.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Increasing">
				<xs:annotation>
					<xs:documentation>Becoming larger in magnitude.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Decreasing">
				<xs:annotation>
					<xs:documentation>Becoming smaller in magnitude.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Strong">
				<xs:annotation>
					<xs:documentation>Not easily broken or destroyed.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Good">
				<xs:annotation>
					<xs:documentation>In a satisfactory condition to use.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Moderately">
				<xs:annotation>
					<xs:documentation>Fairly but not very.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Poor">
				<xs:annotation>
					<xs:documentation>Not as good as it could be or should.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Buoyed">
				<xs:annotation>
					<xs:documentation>Marked by buoys.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Fully Operational">
				<xs:annotation>
					<xs:documentation>Entire observation platform is operating in accordance with, or exceeding, manufacturer specifications.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Partially Operational">
				<xs:annotation>
					<xs:documentation>At least one instrument that is part of an observation platform is not operating to manufacturer specification.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Drifting">
				<xs:annotation>
					<xs:documentation>Floating platform at the mercy of environmental elements, whether intentional or not.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Broken">
				<xs:annotation>
					<xs:documentation>Fractured or in pieces.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Offline">
				<xs:annotation>
					<xs:documentation>Observation platform is intentionally not reporting an environmental observation.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Discontinued">
				<xs:annotation>
					<xs:documentation>Observation station, suite of instruments, or an individual instrument, for a particular location, has been removed and is no longer at the particular location.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Manual Observation">
				<xs:annotation>
					<xs:documentation>Observations made by a human observer.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Unknown Status">
				<xs:annotation>
					<xs:documentation>Status of an observation platform, suite of instruments, or individual instrument is not known or unspecified.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Confirmed">
				<xs:annotation>
					<xs:documentation>Made certain as to truth, accuracy, validity, availability, etc.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Candidate">
				<xs:annotation>
					<xs:documentation>Item selected for an action.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Under Modification">
				<xs:annotation>
					<xs:documentation>Item that is in the process of being modified.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Under Removal / Deletion">
				<xs:annotation>
					<xs:documentation>Item in the process of being removed or deleted.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Removed / Deleted">
				<xs:annotation>
					<xs:documentation>Item that has been removed or deleted.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Candidate for Modification">
				<xs:annotation>
					<xs:documentation>Item selected for modification.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="restrictionType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Anchoring Prohibited">
				<xs:annotation>
					<xs:documentation>An area within which anchoring is not permitted.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Anchoring Restricted">
				<xs:annotation>
					<xs:documentation>A specified area designated by appropriate authority, within which anchoring is restricted in accordance with certain specified conditions.</xs:documentation>
				</xs:annotation>
