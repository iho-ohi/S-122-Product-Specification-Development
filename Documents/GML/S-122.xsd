	<xs:complexType name="textContentType">
		<xs:annotation>
			<xs:documentation>Textual material, or a pointer to a resource providing textual material. May be accompanied by basic information about its source and relationship to the source.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="categoryOfText" type="categoryOfTextType" minOccurs="0" maxOccurs="1">
